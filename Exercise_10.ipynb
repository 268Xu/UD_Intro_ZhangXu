   "source": [
    "Write a piece of code that prints the first n numbers of the padovan sequence"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "5cf3270e",
   "metadata": {},
   "outputs": [],
   "source": [
    "n = int(input())\n",
    "\n",
    "padovan = [1, 1, 1]\n",
    "\n",
    "if n < 3:\n",
    "    print(padovan[:n])\n",
    "else:\n",
    "    for i in range(3, n):\n",
    "        a = padovan[i - 3] + padovan[i - 2]\n",
    "        padovan.append(a)\n",
    "print(padovan)"
   ]
  }
