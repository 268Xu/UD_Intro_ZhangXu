   "source": [
    "Write a piece of code that prints the first n numbers of the padovan sequence"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "id": "5cf3270e",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "50\n",
      "[1, 1, 1, 2, 2, 3, 4, 5, 7, 9, 12, 16, 21, 28, 37, 49, 65, 86, 114, 151, 200, 265, 351, 465, 616, 816, 1081, 1432, 1897, 2513, 3329, 4410, 5842, 7739, 10252, 13581, 17991, 23833, 31572, 41824, 55405, 73396, 97229, 128801, 170625, 226030, 299426, 396655, 525456, 696081]\n"
     ]
    }
   ],
   "source": [
    "n = int(input())\n",
    "\n",
    "padovan = [1, 1, 1]\n",
    "\n",
    "if n < 3:\n",
    "    print(padovan[:n])\n",
    "else:\n",
    "    for i in range(3, n):\n",
    "        a = padovan[i - 3] + padovan[i - 2]\n",
    "        padovan.append(a)\n",
    "print(padovan)"
   ]
